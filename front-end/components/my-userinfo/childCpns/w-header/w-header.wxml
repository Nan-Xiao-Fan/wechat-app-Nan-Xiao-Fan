<!--pages/profile/childCpns/w-header/w-header.wxml-->
<view class='header'>
  <view class='avatar'>
    <image src="{{userInfo.avatarUrl}}"/>
  </view>
  <view class='login-info'>
    <view class='login'>{{userInfo.nickName}}</view>
    <view class='phone'>
      <image src="/assets/profile/phone.png"/>
      {{userInfo.phone}}
    </view>
  </view>
  <view class='arrow'>
    <image src="/assets/common/arrow-right.png"/>
  </view>
</view>
